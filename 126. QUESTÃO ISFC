#include <stdio.h>

int main() {
    int A[10], B[10];

    for (int i = 0; i < 10; i++) {
        printf("Digite A[%d]: ", i);
        scanf("%d", &A[i]);
        B[i] = A[i]; // Cópia
    }

    printf("Vetor B (cópia): ");
    for (int i = 0; i < 10; i++) {
        printf("%d ", B[i]);
    }

    return 0;
}
