#include <stdio.h>

int main() {
    int mat[2][3], soma = 0;

    for (int i = 0; i < 2; i++)
        for (int j = 0; j < 3; j++) {
            printf("mat[%d][%d]: ", i, j);
            scanf("%d", &mat[i][j]);
            soma += mat[i][j];
        }

    printf("Soma dos elementos: %d\n", soma);

    return 0;
}
