#include <stdio.h>

int main() {
    int A[10], B[10], C[10];

    for (int i = 0; i < 10; i++) {
        printf("Digite A[%d]: ", i);
        scanf("%d", &A[i]);
    }
    for (int i = 0; i < 10; i++) {
        printf("Digite B[%d]: ", i);
        scanf("%d", &B[i]);
        C[i] = A[i] + B[i];
    }

    printf("Vetor C (soma): ");
    for (int i = 0; i < 10; i++) {
        printf("%d ", C[i]);
    }

    return 0;
}
