#include <stdio.h>

int main() {
    int A[5], B[5], C[10];

    for (int i = 0; i < 5; i++) {
        printf("Digite A[%d]: ", i);
        scanf("%d", &A[i]);
        C[i] = A[i];
    }
    for (int i = 0; i < 5; i++) {
        printf("Digite B[%d]: ", i);
        scanf("%d", &B[i]);
        C[i + 5] = B[i];
    }

    printf("Vetor C (uniÃ£o): ");
    for (int i = 0; i < 10; i++) {
        printf("%d ", C[i]);
    }

    return 0;
}
